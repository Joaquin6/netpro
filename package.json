    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "js-yaml": "^3.13.1",
    "pre-commit": "^1.2.2"
